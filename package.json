{
  "name": "@voxpelli/node-cli-template",
  "version": "0.0.0",
  "description": "A GitHub template repo for node cli tools",
  "homepage": "http://github.com/voxpelli/node-cli-template",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-cli-template.git"
  },
  "keywords": [],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "MIT",
  "engines": {
    "node": "^20.9.0 || >=21.1.0"
  },
  "bin": {
    "node-cli-template": "cli.js"
  },
  "type": "module",
  "files": [
    "cli.js",
    "lib/**/*.js"
  ],
  "scripts": {
    "check:installed-check": "installed-check",
    "check:knip": "knip",
    "check:lint": "eslint --report-unused-disable-directives .",
    "check:tsc": "tsc",
    "check:type-coverage": "type-coverage --detail --strict --at-least 99 --ignore-files 'test/*'",
    "check": "run-p check:*",
    "prepare": "husky",
    "test": "run-s check"
  },
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.7",
    "@types/node": "^18.19.39",
    "@voxpelli/eslint-config": "^20.0.0",
    "@voxpelli/tsconfig": "^13.0.0",
    "c8": "^10.1.2",
    "chai": "^4.4.1",
    "eslint": "^9.7.0",
    "husky": "^9.0.11",
    "installed-check": "^9.3.0",
    "knip": "^5.26.0",
    "mocha": "^10.6.0",
    "npm-run-all2": "^6.2.2",
    "type-coverage": "^2.29.1",
    "typescript": "~5.5.3"
  },
  "dependencies": {
    "@voxpelli/typed-utils": "^1.10.2",
    "markdown-or-chalk": "^0.2.1",
    "peowly": "^1.3.2",
    "pony-cause": "^2.1.11"
  }
